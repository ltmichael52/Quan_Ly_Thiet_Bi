@model Phong

<h1>Create New Phòng</h1>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="Map" class="control-label"></label>
        <input asp-for="Map" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Macs" class="control-label"></label>
        <input asp-for="Macs" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Tenphong" class="control-label"></label>
        <input asp-for="Tenphong" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Loaiphong" class="control-label"></label>
        <input asp-for="Loaiphong" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Douutien" class="control-label"></label>
        <input asp-for="Douutien" class="form-control" />
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>
